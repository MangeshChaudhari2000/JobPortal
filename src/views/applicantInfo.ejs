<table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">ApplicantID</th>
      <th scope="col">Name</th>
      <th scope="col">Email</th>
      <th scope="col">Contact</th>
      <th scope="col">Resume</th>
    </tr>
  </thead>
  <tbody>
    <% appInfo.forEach((data)=>{%>
    <tr>
      <th scope="row"><%=data.applicantid%></th>
      <td><%=data.name%></td>
      <td><%=data.email%></td>
      <td><%=data.contact%></td>
      <td>
        <a href="<%=data.resumePath%>"
          ><button type="button" class="btn btn-primary">view</button></a
        >
      </td>
    </tr>

    <% })%>
  </tbody>
</table>
